    }
  }
  
  async function fetchMultipleMonths() {
    const allEvents = [];
    const today = new Date();
    
    // ä»Šæœˆã‹ã‚‰4ãƒ¶æœˆå…ˆã¾ã§å–å¾—ï¼ˆååˆ†ãªãƒ‡ãƒ¼ã‚¿ç¢ºä¿ï¼‰
    for (let i = 0; i < 5; i++) {
      const targetDate = new Date(today.getFullYear(), today.getMonth() + i, 1);
      const monthStr = `${targetDate.getFullYear()}-${String(targetDate.getMonth() + 1).padStart(2, '0')}`;
      
      try {
        const url = `${FUNC_BASE}/get-events?month=${monthStr}`;
        const response = await fetch(url, {
          headers: { Authorization: `Bearer ${ANON_KEY}` }
        });
        
        if (response.ok) {
          const data = await response.json();
          const events = data.events || data.data || [];
          allEvents.push(...events);
          console.log(`ğŸ“… ${monthStr}: ${events.length}ä»¶å–å¾—`);
        } else {
          console.warn(`âš ï¸ ${monthStr}: APIå¿œç­”ã‚¨ãƒ©ãƒ¼ ${response.status}`);
        }
      } catch (error) {
        console.warn(`âš ï¸ ${monthStr}ã®ãƒ‡ãƒ¼ã‚¿å–å¾—ã«å¤±æ•—:`, error);
      }
    }
    
    return allEvents;
  }
  
  function removeDuplicates(events) {
    // åŒä¸€æ—¥ä»˜å†…ã§ã®slugé‡è¤‡é™¤å»ï¼ˆæœŸé–“ã‚¤ãƒ™ãƒ³ãƒˆä¿è­·ï¼‰
    const byDate = {};
    events.forEach(event => {
      const date = event.event_date;
      if (!byDate[date]) byDate[date] = [];
      byDate[date].push(event);
    });
    
    const cleaned = [];
    Object.keys(byDate).forEach(date => {
      const dayEvents = byDate[date];
      const seenSlugs = new Set();
      const uniqueDayEvents = [];
      
      dayEvents.forEach(event => {
        if (!seenSlugs.has(event.slug)) {
          seenSlugs.add(event.slug);
          uniqueDayEvents.push(event);
        }
      });
      
      cleaned.push(...uniqueDayEvents);
    });
    
    return cleaned;
  }
  
  function countEventsByRegion(events) {
    const counts = {};
    
    // åˆæœŸåŒ–ï¼ˆ0ä»¶ã‚‚è¡¨ç¤ºã™ã‚‹ãŸã‚å…¨åœ°åŸŸã‚’åˆæœŸåŒ–ï¼‰
    Object.keys(regionConfig).forEach(region => {
      counts[region] = { total: 0, areas: {} };
      regionConfig[region].areas.forEach(area => {
        counts[region].areas[area.code] = 0;
      });
    });
    
    // ã‚¤ãƒ™ãƒ³ãƒˆã‚’ã‚«ã‚¦ãƒ³ãƒˆ
    events.forEach(event => {
      const regions = event.regions || [];
      
      regions.forEach(region => {
        // åºƒåŸŸåœ°åŸŸï¼ˆKANTOã€KANSAIç­‰ï¼‰ã¸ã®ç›´æ¥ãƒãƒƒãƒ”ãƒ³ã‚°
        if (counts[region]) {
          counts[region].total++;
        }
        
        // å€‹åˆ¥éƒ½é“åºœçœŒãƒ»ã‚¨ãƒªã‚¢ã¸ã®ãƒãƒƒãƒ”ãƒ³ã‚°
        Object.keys(regionConfig).forEach(regionKey => {
          const areaExists = regionConfig[regionKey].areas.find(area => area.code === region);
          if (areaExists) {
            counts[regionKey].areas[region]++;
            counts[regionKey].total++;
          }
        });
      });
    });
    
    console.log('ğŸ“Š åœ°åŸŸåˆ¥é›†è¨ˆçµæœ:', counts);
    return counts;
  }
  
  function renderRegionCards(counts) {
    const grid = document.getElementById('region-layout-grid');
    
    const html = Object.keys(regionConfig).map(regionKey => {
      const region = regionConfig[regionKey];
      const regionCount = counts[regionKey];
      
      // ã™ã¹ã¦ã®åœ°åŸŸã‚’è¡¨ç¤ºï¼ˆ0ä»¶ã‚‚å«ã‚€ï¼‰
      const areaLinks = region.areas.map(area => {
        const count = regionCount.areas[area.code];
        const cssClass = count === 0 ? 'region-link-item zero-count' : 'region-link-item';
        
        return `
          <a href="${area.link}" class="${cssClass}">
            <span>${area.name}</span>
            <span>${count}</span>
            <span>&gt;</span>
          </a>
        `;
      }).join('');
      
      return `
        <div class="region-group-card">
          <a href="${region.allLink}" class="region-group-header">
            <h3>${region.titleEn}</h3>
            <span>${regionCount.total}</span>
          </a>
          <div class="region-detail-links">
            ${areaLinks}
          </div>
        </div>
      `;
    }).join('');
    
    grid.innerHTML = html;
    
    // è¡¨ç¤ºåˆ‡æ›¿
    document.getElementById('region-loading').style.display = 'none';
    grid.style.display = 'grid';
    document.getElementById('all-regions-button').style.display = 'block';
    
    console.log('ğŸ¨ åœ°åŸŸã‚«ãƒ¼ãƒ‰æç”»å®Œäº†');
  }
  
