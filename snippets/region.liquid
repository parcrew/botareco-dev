      const uniqueEvents = removeDuplicates(futureEvents);
      console.log(`ğŸ”§ é‡è¤‡é™¤å»å¾Œ: ${uniqueEvents.length}ä»¶`);
      
      // åœ°åŸŸåˆ¥é›†è¨ˆ
